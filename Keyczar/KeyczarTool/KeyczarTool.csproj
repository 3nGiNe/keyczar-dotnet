<Project>
  <Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props" />
  <PropertyGroup>
    <TargetFramework>net40</TargetFramework>
    <OutputType>Exe</OutputType>
    <Description>Keyczar dotnet</Description>
    <Company></Company>
    <Authors> jbtule </Authors>
    <Copyright>Copyright © Contributors 2012-2017"</Copyright>
    <AssemblyVersion>0.7.7.1</AssemblyVersion>
    <FileVersion>0.7.7.1</FileVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="ManyConsole" Version="0.4.2.*" />
    <PackageReference Include="NDesk.Options" Version="0.2.*" />
    <PackageReference Include="DiminishDependencies.Tool" Version="1.1.3" PrivateAssets="All" />
 </ItemGroup>
 <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Numerics" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Keyczar\Keyczar.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Localized.Designer.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>Localized.resx</DependentUpon>
    </Compile>
    <EmbeddedResource Update="Localized.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Localized.Designer.cs</LastGenOutput>
      <SubType>Designer</SubType>
    </EmbeddedResource>
  </ItemGroup>

   <Import Sdk="Microsoft.NET.Sdk" Project="Sdk.targets" />


  <PropertyGroup>
    <PostBuildEvent>"$(ProjectDir)\..\packages\diminishdependencies.tool\1.1.3\tools\DiminishDependencies.exe" -o "$(ProjectDir)\..\KeyczarTool.Minified\Diminish\refs" "$(TargetDir)KeyczarTool.exe" "$(TargetDir)*.dll"</PostBuildEvent>
  </PropertyGroup>

</Project>
